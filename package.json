{
  "name": "react-smart-data-table",
  "version": "0.8.0",
  "description": "A smart data table component for React.js meant to be configuration free",
  "main": "dist/react-smart-data-table.js",
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build:test-dev": "NODE_ENV=development webpack --mode development --config webpack.test.js",
    "build:test": "NODE_ENV=production webpack --mode production --config webpack.test.js",
    "build": "NODE_ENV=production webpack --mode production",
    "clean": "rm -rf dist/",
    "dev": "yarn build:dev && yarn build:test-dev",
    "jslint:fix": "eslint . --fix",
    "jslint": "eslint .",
    "lint-staged": "scripts/lint.sh",
    "lint:fix": "yarn jslint:fix && yarn prettier:fix",
    "lint": "yarn jslint && yarn prettier",
    "prd": "yarn build && yarn build:test",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "server": "NODE_ENV=development webpack-dev-server",
    "start": "yarn build && yarn server",
    "test": "jest --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaocarmo/react-smart-data-table.git"
  },
  "keywords": [
    "configuration",
    "data",
    "react",
    "simple",
    "smart",
    "table"
  ],
  "author": "Joao Carmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joaocarmo/react-smart-data-table/issues"
  },
  "homepage": "https://github.com/joaocarmo/react-smart-data-table#readme",
  "dependencies": {
    "escape-string-regexp": "^4.0.0",
    "flat": "^5.0.2",
    "linkifyjs": "^2.1.8",
    "memoize-one": "^5.1.1",
    "snake-case": "^3.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "faker": "^5.1.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "mini-css-extract-plugin": "^0.11.0",
    "prettier": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.1.3"
  }
}
