{
  "name": "react-smart-data-table",
  "version": "0.8.0",
  "description": "A smart data table component for React.js meant to be configuration free",
  "private": false,
  "main": "dist/react-smart-data-table.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "scripts": {
    "build:dev": "NODE_ENV=development webpack",
    "build:example-dev": "NODE_ENV=development webpack --config webpack.dev.js",
    "build:example": "NODE_ENV=production webpack --config webpack.dev.js",
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf dist/",
    "dev": "yarn build:dev && yarn build:example-dev",
    "lint-staged": "scripts/lint.sh",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prd": "yarn build && yarn build:example",
    "prepublishOnly": "pinst --disable",
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "start": "NODE_ENV=development webpack serve --config webpack.dev.js",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaocarmo/react-smart-data-table.git"
  },
  "keywords": [
    "configuration",
    "data",
    "react",
    "simple",
    "smart",
    "table"
  ],
  "author": "Joao Carmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joaocarmo/react-smart-data-table/issues"
  },
  "homepage": "https://github.com/joaocarmo/react-smart-data-table#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "escape-string-regexp": "^4.0.0",
    "flat": "^5.0.2",
    "linkifyjs": "^2.1.8",
    "prop-types": "^15.7.2",
    "snake-case": "^3.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "husky": "^5.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "mini-css-extract-plugin": "^1.3.6",
    "pinst": "^2.1.4",
    "prettier": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.1.3"
  }
}
